<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Start_End_Time_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>!ISBLANK(End__c) &amp;&amp;  End__c &lt;  Start__c + 1</errorConditionFormula>
    <errorDisplayField>End__c</errorDisplayField>
    <errorMessage>End Date Must be After Start Date</errorMessage>
</ValidationRule>
